{
  "name": "vvit/admission-system",
  "description": "College Admission System with PDF, QR Code, Checklist",
  "type": "project",
  "require": {
    "php": ">=8.1",
    "dompdf/dompdf": "^2.0",
    "phpmailer/phpmailer": "^6.9",
    "endroid/qr-code": "^5.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  }
}